{
    "name": "mtrade",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "test": "jest",
        "prisma:generate": "npx dotenv -e .env.local -- prisma generate",
        "prisma:migrate:dev": "npx dotenv -e .env.local -- prisma migrate dev",
        "prisma:studio": "npx dotenv -e .env.local -- prisma studio",
        "prisma:push": "npx dotenv -e .env.local -- prisma db push",
        "prisma:reset": "npx dotenv -e .env.local -- prisma migrate reset",
        "prisma:seed": "npx dotenv -e .env.local -- prisma db seed",
        "production:build": "npx prisma generate && npx prisma migrate deploy && next build",
        "prisma:setup": "npm run prisma:generate && npm run prisma:push",
        "local": "npm run prisma:generate && npm run dev",
        "linux:local": "npm run prisma:generate && sudo docker-compose up -d && npm run dev"
    },
    "dependencies": {
        "@fortawesome/free-brands-svg-icons": "^6.4.0",
        "@fortawesome/free-solid-svg-icons": "^6.4.0",
        "@fortawesome/react-fontawesome": "^0.2.0",
        "@next-auth/prisma-adapter": "^1.0.6",
        "@prisma/client": "^4.16.2",
        "axios": "^1.6.2",
        "bcryptjs": "^2.4.3",
        "countries-list": "^3.0.1",
        "eslint-config-next": "13.4.0",
        "next": "^13.5.6",
        "next-auth": "^4.24.5",
        "next-i18next": "^14.0.0",
        "node-fetch": "^3.3.2",
        "prisma": "^4.16.2",
        "prop-types": "^15.8.1",
        "proptypes": "^1.1.0",
        "qs": "^6.11.2",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "sharp": "^0.33.2",
        "typescript": "5.0.4",
        "ws": "^8.14.2",
        "zod": "^3.21.4"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.2",
        "@types/jest": "^29.5.7",
        "@types/node": "^20.9.0",
        "@types/qs": "^6.9.9",
        "@types/react": "^18.2.15",
        "@types/react-dom": "^18.2.7",
        "@types/supertest": "^2.0.15",
        "@types/ws": "^8.5.9",
        "@typescript-eslint/eslint-plugin": "^5.12.1",
        "cross-env": "^7.0.3",
        "dotenv-cli": "^7.3.0",
        "eslint": "^8.45.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-prettier": "^8.4.0",
        "eslint-plugin-jest": "^27.6.0",
        "eslint-plugin-jsx-a11y": "^6.7.1",
        "eslint-plugin-next": "^0.0.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.27.0",
        "eslint-plugin-react-hooks": "^4.3.0",
        "eslint-plugin-security": "^1.4.0",
        "eslint-plugin-simple-import-sort": "^7.0.0",
        "eslint-plugin-sonarjs": "^0.12.0",
        "husky": "^8.0.3",
        "jest": "^29.7.0",
        "lint-staged": "^13.2.3",
        "node-mocks-http": "^1.13.0",
        "path": "^0.12.7",
        "prettier": "^2.5.1",
        "sass": "^1.64.1",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.1"
    }
}
